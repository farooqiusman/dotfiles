
"""

"""
def validInterpretations(decInput):
	# Write your code here
	marks = []
	for i in range(len(decInput)):
		if i < len(decInput) - 1:
			if decInput[i] == '1':
				marks.append(1)				
				continue
			if decInput[i] == '2' and int(decInput[i + 1]) <= 5:
				marks.append(1)
				continue
		marks.append(0)
	dupes = 0
	mark = 0
	for i in range(len(marks)):
		if marks[i] == 1:
			mark += 1
			if i < len(marks) - 1 and marks[i + 1] == 1:
				dupes += 1
		
		
	return 2 ** mark - dupes

def main():
	#input for decInput
	decInput = str(input())
	
	
	result = validInterpretations(decInput)
	print(result)	

if __name__ == "__main__":
	main()
