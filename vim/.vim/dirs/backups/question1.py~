
"""

"""
def checkAttackTheOpponent(cord_x1, cord_y1, cord_x2, cord_y2):
	# Write your code here
	if cord_x1 == cord_x2:
		return 'Yes'
	
	if cord_y1 == cord_y2:
		return 'Yes'
	
	if abs(cord_x1 - cord_x2) == abs(cord_y1 - cord_y2):
		return 'Yes'

	return 'No'

def main():
	#input for cord_x1
	cord_x1 = int(input())
	
	#input for cord_y1
	cord_y1 = int(input())
	
	#input for cord_x2
	cord_x2 = int(input())
	
	#input for cord_y2
	cord_y2 = int(input())
	
	
	result = checkAttackTheOpponent(cord_x1, cord_y1, cord_x2, cord_y2)
	print(result)	

if __name__ == "__main__":
	main()
